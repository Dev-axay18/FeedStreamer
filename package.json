{
  "name": "react-infinite-feed-gallery",
  "version": "1.0.0",
  "description": "A production-ready, fully responsive, animated infinite scroll image feed component for React",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./styles": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
 "scripts": {
  "build": "rollup -c",
  "dev": "rollup -c -w",
  "clean": "rimraf dist",
  "prebuild": "npm run clean",
  "type-check": "tsc --noEmit",
  "lint": "eslint src --ext .ts,.tsx",
  "prepublishOnly": "npm run build",
  "test": "jest"
},
"jest": {
 "**/__tests__/**/*.[jt]s?(x)",
    "**/tests/**/*.[jt]s?(x)"
},
  "keywords": [
    "react",
    "infinite-scroll",
    "image-gallery",
    "feed",
    "framer-motion",
    "tailwind",
    "typescript",
    "responsive",
    "animated",
    "lazy-loading"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/react-infinite-feed-gallery.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/react-infinite-feed-gallery/issues"
  },
  "homepage": "https://github.com/yourusername/react-infinite-feed-gallery#readme",
  "peerDependencies": {
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "jest": "^29.0.0",
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "postcss-cli": "^10.1.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "rollup-plugin-postcss": "^4.0.0",
    "tailwindcss": "^3.3.0",
    "tslib": "^2.8.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "clsx": "^2.0.0"
  }
}
